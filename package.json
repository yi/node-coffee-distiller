{
  "name": "coffee-distiller",
    "description": "Merge and minify server side coffee-script files with fake CJS wrappers. This tool is useful for developers who want to distribute a SERVER SIDE coffee-script app in the form of single javascript file.",
    "version": "0.1.0",
    "homepage": "https://github.com/yi/node-coffee-distiller",
    "author": {
      "name": "yi",
      "email": "yi2004@gmail.com",
      "url": "https://github.com/yi"
    },
    "bin": {
      "distill": "./bin/coffee-distiller.coffee"
    },
    "repository": {
      "type": "git",
      "url": "git://github.com/yi/node-coffee-distiller.git"
    },
    "bugs": {
      "url": "https://github.com/yi/node-coffee-distiller/issues"
    },
    "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/yi/node-coffee-distiller/blob/master/LICENSE"
    }
  ],
    "main": "./bin/coffee-distiller.coffee",
    "engines": {
      "node": ">= 0.8.0"
    },
    "dependencies": {
      "commander": "latest",
      "underscore": "latest",
      "mkdirp": "latest",
      "debug": "latest",
      "uglify-js2": "latest",
      "walk": "latest"
    },
    "devDependencies": {
    },
    "keywords": ["uglify","build","minify","coffeescript","compiler" ]
}
